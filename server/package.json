{
    "name": "galaxyous-server",
    "version": "1.0.0",
    "description": "Backend server for Galaxyous Union AI Mix",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc && npm run copy-assets",
        "copy-assets": "node -e \"const fs=require('fs');const path=require('path');const src='src/isolation-mode/scenarios/presets';const dest='dist/isolation-mode/scenarios/presets';fs.mkdirSync(dest,{recursive:true});fs.readdirSync(src).forEach(f=>fs.copyFileSync(path.join(src,f),path.join(dest,f)));console.log('Copied scenario presets to dist');\"",
        "start": "node dist/index.js",
        "sync": "ts-node src/scripts/runSync.ts",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "dependencies": {
        "@google/genai": "^1.34.0",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "bullmq": "^5.66.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^8.2.1",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.6.5",
        "node-cron": "^3.0.3",
        "pdf-parse": "^1.1.1",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.3",
        "resend": "^6.6.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "uuid": "^9.0.1",
        "vm2": "^3.10.0",
        "yaml": "^2.8.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.4",
        "@types/node-cron": "^3.0.11",
        "@types/pino": "^7.0.4",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^9.0.8",
        "@types/ws": "^8.18.1",
        "jest": "^30.2.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.6",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    }
}
